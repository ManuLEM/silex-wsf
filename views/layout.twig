<!doctype html>
<html lang="en">
<head>
	<meta charset="UTF-8">
	<title>Document</title>
</head>
<body>
	<header>
		<nav>
			<ul>
				<li><a href="{{ app.url_generator.generate('home') }}">Home</a></li>
				{% if app.session.get('user').name is not defined %}
					<li><a href="{{ app.url_generator.generate('login') }}">Login</a></li>
					<li><a href="{{ app.url_generator.generate('register') }}">Cr√©er un compte</a></li>
				{% elseif app.session.get('user').type == 'admin' %}
					<li><a href="{{ app.url_generator.generate('getAdmin') }}">Admin</a></li>
				{% endif %}
			</ul>
		</nav>
	</header>
	<div id="content">
		{% block content %}{% endblock %}
	</div>
	<footer>
		
	</footer>
</body>
</html>